{
    "name": "sanmai/lazy-linq",
    "type": "library",
    "description": ".NET LINQ-like library with mandatory lazy evaluation",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Alexey Kopytko",
            "email": "alexey@kopytko.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "sanmai/pipeline": "^2 || ^3 || ^4 || ^5"
    },
    "require-dev": {
        "codacy/coverage": "^1.4",
        "friendsofphp/php-cs-fixer": "^2.11",
        "infection/infection": ">=0.8",
        "mockery/mockery": "^1.0",
        "phan/phan": ">=0.11",
        "php-coveralls/php-coveralls": "^2.0",
        "phpstan/phpstan": ">=0.9",
        "phpunit/phpunit": ">=6",
        "vimeo/psalm": ">=1.1"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "LazyLINQ\\": "src/LazyLINQ/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LazyLINQ\\": "tests/LazyLINQ/"
        }
    }
}
